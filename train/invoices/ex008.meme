invoices due_date 2025-03-20;id _;invoice_items invoice_id @;id :cnt:dsc;invoice_id :grp;;

// Average line totals per product on invoices dated 2024-10-05 for active products named like Cable, highest first with product name
invoices invoice_date 2024-10-05;id _;invoice_items invoice_id @;line_total :avg:dsc;product_id :grp;products id @;active 1;name ~"Cable";name _;;

// Customers named Patel with invoices created on 2025-12-12, sort totals low to high
customers name ~"Patel";id _;invoices customer_id @;created 2025-12-12;total :asc;_;;

// On 2024-06-30, maximum quantity per product for items mentioning HDMI, include product names
invoices invoice_date 2024-06-30;id _;invoice_items invoice_id @;description ~"HDMI";quantity :max:dsc;product_id :grp;products id @;name _;;

// Paid invoices on 2025-01-10 with totals 100 to 500, order by invoice number ascending
invoices status paid;total >=100;<=500;invoice_date 2025-01-10;invoice_no :asc;_;;

// Customers created during 2024-01 with phone starting 212-, order by email A-Z
customers created >=2024-01-01;<=2024-01-31;phone ~"212-";email :asc;_;;

// Products priced 10..20 created on 2025-09-09, sort by SKU A-Z
products unit_price >=10;<=20;created 2025-09-09;sku :asc;_;;

// Customer name similar to 'Northwind Trading Inc' on 2025-05-05, threshold 0.7
customers name <=>"Northwind Trading Inc">=0.7;created 2025-05-05;_;;

// Top 5 invoice numbers by average tax in 2024-03
invoices invoice_date >=2024-03-01;<=2024-03-31;tax :avg:dsc;invoice_no :grp;%m lim 5;;

// Counts of open invoices per customer on 2025-08-08, include customer names, highest first
invoices status open;invoice_date 2025-08-08;id :cnt:dsc;customer_id :grp;customers id @;name _;;

// On 2025-02-20, sum of item line totals by invoice, top 10
invoices invoice_date 2025-02-20;id _;invoice_items invoice_id @;line_total :sum:dsc;invoice_id :grp;%m lim 10;;

// Overdue invoices for customer 505 due on 2025-10-01, sort totals high to low
invoices customer_id 505;status overdue;due_date 2025-10-01;total :dsc;_;;

// Items with description including Adapter for invoice INV-4001 issued on 2025-07-15
invoices invoice_no "INV-4001";invoice_date 2025-07-15;id _;invoice_items invoice_id @;description ~"Adapter";_;;

// Active products similar to 'wireless keyboard' created on 2024-02-02 with similarity at least 0.8, order by closeness
products name <=>"wireless keyboard":dsc>=0.8;active 1;created 2024-02-02;_;;

// Invoice days in 2025-01 ranked by counts where counts are at least 20
invoices invoice_date >=2025-01-01;<=2025-01-31;id :cnt:dsc>=20;invoice_date :grp;;

// For customers with emails ending @contoso.com, first 15 invoices on 2025-10-10 sorted by issue date ascending
customers email ~"@contoso.com";id _;invoices customer_id @;invoice_date 2025-10-10;invoice_date :asc;_;%m lim 15;;

// Average quantity per product for SKUs starting XYZ on 2025-12-01, highest first
invoices invoice_date 2025-12-01;id _;invoice_items invoice_id @;quantity :avg:dsc;product_id :grp;products id @;sku ~"XYZ";name _;;

