// follows follower_id at least 100, followee_id at least 10
follows follower_id >=100;followee_id >=10;_;;

// follows id at least 42, created_at at least 2024-01-15, followee_id at most 1000
follows id >=42;created_at >=2024-01-15;followee_id <=1000;_;;

// follows follower_id at least 100, created_at at least 2024-01-15, sort by id asc
follows follower_id >=100;created_at >=2024-01-15;id :asc;_;;

// follows followee_id at most 1000, id at least 10, sort by created_at dsc
follows followee_id <=1000;id >=10;created_at :dsc;_;;

// follows follower_id at least 100, sort by created_at dsc with limit 5
follows follower_id >=100;created_at :dsc;_;%m lim 5;;

// follows followee_id at most 1000, sort by id asc with limit 10
follows followee_id <=1000;id :asc;_;%m lim 10;;

// follows id at least 42, sort by created_at asc with limit 5
follows id >=42;created_at :asc;_;%m lim 5;;

// follows follower_id at least 100, sort by created_at dsc with limit 10 offset 20
follows follower_id >=100;created_at :dsc;_;%m lim 10;beg 20;;

// follows followee_id at most 1000, sort by id asc with limit 10 offset 30
follows followee_id <=1000;id :asc;_;%m lim 10;beg 30;;

// follows id at least 10, sort by created_at asc with limit 5 offset 15
follows id >=10;created_at :asc;_;%m lim 5;beg 15;;

