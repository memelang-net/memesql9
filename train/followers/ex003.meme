users updated_at :asc;_;;

// users by id descending
users id :dsc;_;;

// users by username descending
users username :dsc;_;;

// users by email descending
users email :dsc;_;;

// users by password_hash descending
users password_hash :dsc;_;;

// users by created_at descending
users created_at :dsc;_;;

// users by updated_at descending
users updated_at :dsc;_;;

// users by id asc then created_at dsc
users id :asc;created_at :dsc;_;;

// users by username dsc then updated_at asc
users username :dsc;updated_at :asc;_;;

// users by created_at asc then updated_at dsc then id asc
users created_at :asc;updated_at :dsc;id :asc;_;;

// users id from 10 to 250
users id >=10;<=250;_;;

// users created_at from 2024-01-15 to 2025-07-04
users created_at >=2024-01-15;<=2025-07-04;_;;

// users updated_at from 2023-10-10 to 2025-01-01
users updated_at >=2023-10-10;<=2025-01-01;_;;

// users username contains ana, email contains .org
users username ~"ana";email ~".org";_;;

// users email not containing spam, created_at at least 2024-01-15, updated_at at most 2025-10-01
users email !~"spam";created_at >=2024-01-15;updated_at <=2025-10-01;_;;

// users id at least 42, username contains dev, email contains .com, updated_at at most 2025-10-01
users id >=42;username ~"dev";email ~".com";updated_at <=2025-10-01;_;;

// users username contains dev, created_at at least 2024-01-15, sort by id asc
users username ~"dev";created_at >=2024-01-15;id :asc;_;;

// users email contains .com, updated_at at most 2025-10-01, sort by created_at dsc
users email ~".com";updated_at <=2025-10-01;created_at :dsc;_;;

// users username contains dev, created_at at least 2024-01-15, sort by id dsc with limit 5
users username ~"dev";created_at >=2024-01-15;id :dsc;_;%m lim 5;;

// users email contains .com, updated_at at most 2025-10-01, sort by created_at asc with limit 10
users email ~".com";updated_at <=2025-10-01;created_at :asc;_;%m lim 10;;

// users username contains ana, id at least 100, sort by updated_at dsc with limit 5
users username ~"ana";id >=100;updated_at :dsc;_;%m lim 5;;

// users username contains dev, created_at at least 2024-01-15, sort by id asc with limit 10 offset 20
users username ~"dev";created_at >=2024-01-15;id :asc;_;%m lim 10;beg 20;;

// users email contains .org, updated_at at most 2025-10-01, sort by created_at dsc with limit 10 offset 30
users email ~".org";updated_at <=2025-10-01;created_at :dsc;_;%m lim 10;beg 30;;

// users id at least 42, username contains ana, sort by updated_at asc with limit 5 offset 15
users id >=42;username ~"ana";updated_at :asc;_;%m lim 5;beg 15;;

