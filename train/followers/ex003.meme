// profiles id from 10 to 250
profiles id >=10;<=250;_;;

// profiles user_id from 42 to 2500
profiles user_id >=42;<=2500;_;;

// profiles birthday from 1990-05-15 to 2005-12-31
profiles birthday >=1990-05-15;<=2005-12-31;_;;

// profiles created_at from 2024-01-15 to 2025-07-04
profiles created_at >=2024-01-15;<=2025-07-04;_;;

// profiles updated_at from 2023-10-10 to 2025-01-01
profiles updated_at >=2023-10-10;<=2025-01-01;_;;

// profiles full_name contains Smith, location contains York
profiles full_name ~"Smith";location ~"York";_;;

// profiles website contains .org, avatar_url not containing .gif, bio contains travel
profiles website ~".org";avatar_url !~".gif";bio ~"travel";_;;

// profiles id at least 100, user_id at least 10, location contains San, updated_at at most 2025-10-01
profiles id >=100;user_id >=10;location ~"San";updated_at <=2025-10-01;_;;

// profiles location contains Austin, created_at at least 2024-01-15, sort by full_name asc
profiles location ~"Austin";created_at >=2024-01-15;full_name :asc;_;;

// profiles website contains .com, updated_at at most 2025-10-01, sort by created_at dsc
profiles website ~".com";updated_at <=2025-10-01;created_at :dsc;_;;

// profiles bio contains engineer, location contains York, sort by updated_at dsc with limit 5
profiles bio ~"engineer";location ~"York";updated_at :dsc;_;%m lim 5;;

// profiles full_name contains Jane, website contains .io, sort by created_at asc with limit 10
profiles full_name ~"Jane";website ~".io";created_at :asc;_;%m lim 10;;

// profiles id at least 42, birthday at most 2005-12-31, sort by updated_at dsc with limit 5
profiles id >=42;birthday <=2005-12-31;updated_at :dsc;_;%m lim 5;;

// profiles location contains San, created_at at least 2024-01-15, sort by full_name asc with limit 10 offset 20
profiles location ~"San";created_at >=2024-01-15;full_name :asc;_;%m lim 10;beg 20;;

// profiles website contains .org, updated_at at most 2025-10-01, sort by created_at dsc with limit 10 offset 30
profiles website ~".org";updated_at <=2025-10-01;created_at :dsc;_;%m lim 10;beg 30;;

// profiles id at least 100, location contains Austin, sort by updated_at asc with limit 5 offset 15
profiles id >=100;location ~"Austin";updated_at :asc;_;%m lim 5;beg 15;;

